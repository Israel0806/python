import math
def distancia(n):
    l1=[]
    l2=[]
    suma=0
    for x in range(0,n):
        print "Ingrese el primer punto_",x+1,":"
        a=input()
        l1.append(a)
    for y in range(0,n):
        print "Ingrese el segundo punto_",y+1,":"
        b=input()
        l2.append(b)
    for z in range(0,n):
        suma+=(l1[z]-l2[z])*(l1[z]-l2[z])
    print "La distancia euclidiana es: ",math.sqrt(suma)

A1=input("Ingrese cuantos elementos tendran los puntos: ")
A=distancia(A1)
